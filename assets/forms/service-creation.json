                                "text1": "18:00",
                                "text2": "18:00",
                                "text3": "18:00",
                                "text4": "18:00",
                                "text5": "18:00"
                            },
                            "isRequired": true,
                            "validators": [
                                {
                                    "type": "expression"
                                }
                            ],
                            "items": [
                                {
                                    "name": "text1",
                                    "inputType": "time",
                                    "title": "Monday"
                                },
                                {
                                    "name": "text2",
