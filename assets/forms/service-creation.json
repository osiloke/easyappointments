{
    "title": "List Your Services",
    "description": "Create Listing your services to start receiving bookings and managing your appointments on SchedlMe.",
    "loadingHtml": "<h3>Loading form...</h3>",
    "pages": [
        {
            "name": "listing_details",
            "elements": [
                {
                    "type": "text",
                    "name": "package_name",
                    "title": "Enter a listing name",
                    "isRequired": true,
                    "validators": [
                        {
                            "type": "expression"
                        }
                    ],
                    "placeholder": "Ex: One-on-One Consultation "
                },
                {
                    "type": "comment",
                    "name": "package_description",
                    "title": "Listing Description",
                    "isRequired": true,
                    "maxLength": 250,
                    "autoGrow": true
                },
                {
                    "type": "file",
                    "name": "package_image",
                    "title": "Select Listing Image",
                    "waitForUpload": true,
                    "maxSize": 2000000,
                    "filePlaceholder": "Drag and drop a file here or click the button below to select a file to upload. (Max size 2MB)"
                }
            ],
            "title": "Listing Details",
            "description": "Listings segment the various services that you offer and so that they can be viewed as a groups where necessary and booked by your customers."
        },
        {
            "name": "availability",
            "elements": [
